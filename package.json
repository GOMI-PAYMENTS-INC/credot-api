    "@prisma/client": "^5.4.2",
    "@types/lodash": "^4.14.201",
    "@types/react-dom": "^18.2.14",
    "axios": "^1.5.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.4",
    "cache-manager": "^5.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "dd-trace": "^5.5.0",
    "exceljs": "^4.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "googleapis": "^129.0.0",
