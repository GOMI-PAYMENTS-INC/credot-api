{
  "name": "gomi-settlement-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:client": "nest build client",
    "build:office": "nest build office",
    "build:consumer": "nest build consumer",
    "build:batch": "nest build batch",
    "start:client": "TZ=UTC nest start client --watch",
    "start:office": "TZ=UTC nest start office --watch",
    "start:consumer": "TZ=UTC nest start consumer --watch",
    "start:batch": "TZ=UTC nest start batch --watch",
    "----": "------------------------------------------------------------",
    "start:client:dev": "PORT=9000 NODE_ENV=dev dotenv -e .env.dev npm run start:client",
    "start:office:dev": "PORT=9005 NODE_ENV=dev dotenv -e .env.dev npm run start:office",
    "start:consumer:dev": "PORT=9001 NODE_ENV=dev dotenv -e .env.dev npm run start:consumer",
    "start:batch:dev": "PORT=9002 NODE_ENV=dev dotenv -e .env.dev npm run start:batch",
    "--------": "------------------------------------------------------------",
    "start:client:prod": "NODE_ENV=prod npm run start:client",
    "start:office:prod": "NODE_ENV=prod npm run start:office",
    "start:consumer:prod": "NODE_ENV=prod npm run start:consumer",
    "start:batch:prod": "NODE_ENV=prod npm run start:batch",
    "------": "------------------------------------------------------------",
    "start:client:local": "NODE_ENV=local dotenv -e .env.local npm run start:client",
    "start:office:local": "NODE_ENV=local dotenv -e .env.local npm run start:office",
    "start:consumer:local": "NODE_ENV=local dotenv -e .env.local npm run start:consumer",
    "start:batch:local": "NODE_ENV=local dotenv -e .env.local npm run start:batch",
    "-------": "------------------------------------------------------------",
    "start:debug:office:dev": "NODE_ENV=dev TZ=UTC nest start office --debug --watch",
    "----------": "------------------------------------------------------------",
    "migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate dev",
    "migrate:prod": "npx prisma migrate deploy",
    "test-divider": "--------------------test------------------------",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/test/test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.433.0",
